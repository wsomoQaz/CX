local UILib = loadstring(game:HttpGet("https://raw.githubusercontent.com/wsomoQaz/CX/main/main"))()

local win = UILib:Window("测试窗口")
UILib:Button(win, "点我", function() print("按钮被点了") end)
UILib:Toggle(win, "开关", false, function(state) print("开关状态:", state) end)
UILib:Slider(win, "滑块", 0, 100, 50, function(val) print("滑块值:", val) end)
UILib:Dropdown(win, "下拉菜单", {"选项1","选项2","选项3"}, function(opt) print("选择了:", opt) end)